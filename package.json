{
  "name": "experiments",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "bun-types": "^0.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "gen:resolve": "bun run ./src/scripts/resolveRefs.ts",
